<%- include('partials/head') %>



<body>
    <%- include('partials/navbar') %> 

    <h1>
        Random number generated is =
        <%= rand%>
    </h1>
    <% if(rand % 2==0){ %>
        <h2>This is an even number</h2>
    <% } else { %>
        <h2>This is an odd number</h2>
    <% }%>

    <h3>
        <%= rand % 2 === 0? 'Even' : 'Odd' %>
    </h3>

</body>

</html>